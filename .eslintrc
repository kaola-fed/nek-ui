{
  "extends": "airbnb-base",
  "rules": {
    "func-names": 0,
    "radix": 0,
    "max-len": 0,
    "no-prototype-builtins": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "no-restricted-syntax": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "no-unused-expressions": [
      "error",
      {
        "allowShortCircuit": true,
        "allowTernary": true
      }
    ],
    "no-return-assign": 0
  },
  "globals": {
    "window": true,
    "document": true,
    "FormData": true,
    "File": true,
    "XMLHttpRequest": true,
    "location": true
  }
}